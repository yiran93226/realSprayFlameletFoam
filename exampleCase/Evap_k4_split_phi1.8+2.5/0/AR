FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      AR;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.01378;

boundaryField
{
    AXIS
    {
        type            empty;
    }
    OUTLET
    {
        type            calculated;
        value           uniform 0.01378;
    }
    COFLOW
    {
        type            calculated;
        value           uniform 0.01378;
    }
    PILOT
    {
        type            calculated;
        value           uniform 0.01378;
    }
    FUEL
    {
        type            calculated;
        value           uniform 0.01378;
    }
    WALL
    {
        type            calculated;
        value           uniform 0.01378;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}


